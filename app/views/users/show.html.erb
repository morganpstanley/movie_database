<%= @user.username %> <br>
<%= @user.email %> 

<br><br>

<% if @user == current_user %>
    <%= link_to "See Your Movies", user_movies_path(@user) %> <br>
    <%= link_to "Edit Your profile", edit_user_path(@user) %>
<% else %>
    <%= link_to "See #{@user.username}'s Movies", user_movies_path(@user) %> <br>
<% end %>