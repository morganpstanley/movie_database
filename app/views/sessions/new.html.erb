<h1> Log in </h1>
<%= form_with model: @user, url: login_path, type: :post do |f| %>
    Username: <%= f.text_field :username %> <br>
    Password: <%= f.text_field :password %> <br>
    Confirm Password: <%= f.text_field :password_confirmation %> <br>
    <%= f.submit "Log in"%>
<% end %>

<br>

<%= login_through_github %>